<Window xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Chose template name"
        Width="351.148" ResizeMode="NoResize" SizeToContent="Height" Height="271.112" Background="#FFDEFFEB" BorderBrush="#FF138293">
    <StackPanel Margin="10,10,10,21">
        <Label x:Name="label" Content="Choose template" HorizontalAlignment="Left" Height="36" Width="325"/>
        <ComboBox x:Name="pole" Height="31" ItemsSource="{Binding templates}" DisplayMemberPath="Name" SelectedIndex="0"/>
        <Button x:Name="button" Content="Button" HorizontalAlignment="Left" Height="45" Width="325" Click="sel"/>
    </StackPanel>
</Window>